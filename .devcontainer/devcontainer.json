{
  "name": "${localWorkspaceFolderBasename}",
  "dockerComposeFile": ["docker-compose.yaml"],
  "workspaceFolder": "/workspace",
  "service": "protect",
  "overrideCommand": false,
  "customizations": {
    "vscode": {
      "extensions": [
        "Halon.vscode-halon",
        "Halon.hsl-syntax",
        "Halon.hsl-linter",
        "Halon.hsl-debug",
        "github.vscode-github-actions"
      ]
    }
  }
}